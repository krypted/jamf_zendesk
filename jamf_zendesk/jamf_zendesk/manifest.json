{
  "author": {
    "name": "Jamf",
    "email": "jonathan.yuresko@jamf.com",
    "url": "https://github.com/jamf"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html"
    }
  },
  "version": "1.0",
  "frameworkVersion": "2.0",
  "domainWhitelist": ["{{setting.domain}}"],
  "parameters": [
      {
          "name": "domain",
          "type": "text",
          "required": true,
          "secure": false,
          "default": "yourcompany.com"
      },
      {
          "name": "fullUrl",
          "type": "text",
          "required": true,
          "secure": false,
          "default": "https://jss.yourcompany.com:8443"
      },
      {
          "name": "token",
          "type": "text",
          "secure": true,
          "required": true
      },
      {
          "name": "macOSEA1",
          "type": "text",
          "required": false,
          "secure": false,
          "default": "1"
      },
      {
          "name": "macOSEA2",
          "type": "text",
          "required": false,
          "secure": false,
          "default": "2"
      },
      {
          "name": "macOSEA3",
          "type": "text",
          "required": false,
          "secure": false,
          "default": "3"
      }
  ]
}
