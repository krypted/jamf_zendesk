{
    "name": "Jamf Integration",
    "author": {
        "name": "None",
        "email": "krypted@me.com",
        "url": "https://github.com/krypted/jamf_zendesk"
      },
    "defaultLocale": "en",
    "private": true,
    "location": {
        "support": {
            "ticket_sidebar": "assets/iframe.html"
        }
    },
    "version": "1.0",
    "frameworkVersion": "2.0",
    "domainWhitelist": ["{{setting.subdomain}}.jamfcloud.com"],
    "parameters": [
        {
            "name": "subdomain",
            "type": "text",
            "required": true,
            "secure": true,
            "default": "yourcompany"
        },
        {
            "name": "token",
            "type": "text",
            "secure": true,
            "required": true
        }
    ]
}
